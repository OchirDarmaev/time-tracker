{
  "name": "timetrack",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && yarn build:css && tsx src/server.ts",
    "dev:watch": "tsc --watch & postcss ./src/styles/input.css -o ./public/styles/output.css --watch & tsx watch src/server.ts",
    "sandbox": "tsx src/sandbox.ts",
    "sandbox:watch": "tsc --watch & tsx watch src/sandbox.ts",
    "build": "tsc && yarn build:css",
    "build:css": "postcss ./src/styles/input.css -o ./public/styles/output.css --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\""
  },
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "better-sqlite3": "^9.2.2",
    "cookie-parser": "^1.4.6",
    "express": "5.1.0",
    "express-session": "^1.17.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.6",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22"
  }
}
